                  <Font name="Arial Black" size="16.0" />
               </font>
            </Button>
            <TextField fx:id="display" alignment="CENTER_RIGHT" editable="false" focusTraversable="false" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="43.0" prefWidth="183.0" GridPane.columnSpan="4">
