            <Button fx:id="plusBtn" mnemonicParsing="false" onAction="#plusButtonClicked" prefHeight="43.0" prefWidth="43.0" text="+" textAlignment="CENTER" GridPane.columnIndex="3" GridPane.rowIndex="2">
